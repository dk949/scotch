
// Functions

// Normal function
def func0(): i32 {
    return 0i;
}

// void function
def func0(): void {

}


// return type deduction
def func0() {// deduced as i32
    return 0i;
}

def func0() {// deduced to void
}

// Parameters (value semantics [at least for small types?])
def func0(param0: i32, param1: i64) {
}


// Variables and builtin types types

def main() {
    // Constant declarations
    const var0: i32 = 0i;
    const var1: i64 = 0;
    const var2: i64 = 0l;
    const var3: f32 = 0.0f;
    const var4: f64 = 0.0;
    const var5: f64 = 0.0d;

    // Mutable declaration
    let var6: i32 = 0i;

    // Type deduction
    const var7 = 25; // deduced as i64

    // Strings
    const var8 = "hello";

}


// User defined datastructures

struct S {
    mem0: i32;
    mem1: i64;
}
